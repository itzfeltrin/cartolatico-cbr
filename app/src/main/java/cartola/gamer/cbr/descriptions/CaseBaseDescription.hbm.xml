<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="cartola.gamer.cbr.descriptions.CaseBaseDescription" table="atletas">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="id_rodada" column="id_rodada" />
		<property name="nome" column="nome" />
		<property name="posicao" column="posicao" />
		<property name="status" column="status" />
		<property name="custo" column="custo" />
		<property name="ultima_pontuacao" column="ultima_pontuacao" />
		<property name="media_pontuacao" column="media_pontuacao" />
		<property name="mando" column="mando" />
		<property name="id_oponente" column="id_oponente" />
	</class>
</hibernate-mapping>