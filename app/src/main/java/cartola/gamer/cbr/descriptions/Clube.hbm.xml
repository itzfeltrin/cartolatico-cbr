<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping">
<hibernate-mapping default-lazy="false" package="cartola.gamer.cbr.descriptions">
	<class name="Clube" table="clubes">
		<id name="id" column="id" />
		<property name="nome" column="nome" />
		<property name="abreviacao" column="abreviacao" />
		<property name="nome_fantasia" column="nome_fantasia" />
	</class>
</hibernate-mapping>